#include <iostream>
using namespace std;

struct Student {
    int age;
    string first_name;
    string last_name;
    int standard;
};

int main() {
    Student st;

    cout << "Enter age, first name, last name, and standard:\n";
    cin >> st.age >> st.first_name >> st.last_name >> st.standard;

    cout << st.age << " " << st.first_name << " " << st.last_name << " " << st.standard;

    return 0;
}
